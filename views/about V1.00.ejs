			var alertM = function($m) { 
				$('#AlertModal').modal('show');
				//$('#AlertA').show();
				$("#AlertA").html("<font color='black' ><strong></strong></font>");
				$("#AlertA").append('<center><font color="black" face="trebuchet MS" size="3">' + $m + "</font></center>"); 
