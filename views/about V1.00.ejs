			$(document).on('click', '.btnDelete', function(e){ 
				e.preventDefault();
				
				if($sessionData !=""){
					var btn = $(this);
					btn.button('loading');
					
					var $id = this.id;
					//alertM($id);
					
					var $res = $id.split("_");
					
					var data = {};
					data.emp = $res[1];
					
					data.from = $res[2];
					data.to = $res[3];
					//console.log($sessionData + '  '+ $res[1]);
					if($sessionData == $res[1]){
						$.ajax({
							type: 'POST',
							data: JSON.stringify(data),
							contentType: 'application/json',
							url: '/DelReservation',						
							success: function(data) {
								
								if(data.msg =='NoUser'){
									btn.button('reset');
								}
								else{
								
									//console.log('success');
									btn.button('reset');
									btn.html('Done');
									$divID = 'divCancel' + $res[1]+ $res[2]+$res[3];
									
									//console.log($divID.trim());
									
									$('#'+$divID.trim()).hide();
									$('#'+$divID.trim()).html("<font color='black' ><strong></strong></font>");
									//$('#messageM2Close a').trigger("click");
									//$('#messageModal2').hide();
									//$fGenModal();
									
									
									
									
									
								}
							},
							error: function (error) {
								
								$("#messageM").html("<font color='black' ><strong></strong></font>");
								$("#messageM").append('Error Connecting to Server');
								$('#messageModal').modal('show');
								btn.button('reset');
							}
							
						});	
					}
					else {
						alertM("Authentication Failed");
						btn.button('reset');
						return false;
					}
				}
				else{
					alertM('Please login to cancel the reservation');
					return false;
				}
				
			});
			
			
			
			$('#sendLogin').click(function(e){
				e.preventDefault();
				
				//alert($('#Login-empID').val());
				
				if($('#Login-empID').val() ==""){
					alertM('Please enter Employee ID');
					return false;
				}
				
				if($('#Login-Password').val() == ""){
					alertM('Please enter Password');
					return false;
				}
				var btn = $(this);
				btn.button('loading');
				var data = {};
				data.emp = $('#Login-empID').val();
				data.pass = $('#Login-Password').val();
				
				$.ajax({
					type: 'POST',
					data: JSON.stringify(data),
					contentType: 'application/json',
					url: '/LoginReservation',						
					success: function(data) {
						
						if(data.msg =='Wrong'){
							alertM('Wrong UserID or Password');
							btn.button('reset');
						}
						
						else{
						
							//console.log('success');
							btn.button('reset');
							$('#login').modal('hide');
							$('#Login-group').hide();
							$("#Login-Name").append('<form class="navbar-form navbar-right" role="search">'
									+' <div class="form-group"><font color="white">Welcome! '+ (data.name).split(" ")[0] +" " +'</font><buttton class="btn btn-success btn-sm logout">'
									+ '<font color="white">Logout</font></button></div>'
									+'</form>');
							
							//console.log(data.emp);
							$sessionData = data.emp;
						}
						
						
					},
					error: function (error) {
						btn.button('reset');
						$("#messageM").html("<font color='black' ><strong></strong></font>");
						$("#messageM").append('Error Connecting to Server');
						$('#messageModal').modal('show');
					}
					
				});	
			
			});
			
			
			$(document).on('click', '.logout', function(e){
				
				var data = {};
				
				$.ajax({
					type: 'POST',
					data: JSON.stringify(data),
					contentType: 'application/json',
					url: '/LogoutReservation',						
					success: function(data) {
						//console.log('success');
					
						//$('#login').modal('hide');
						$('#Login-group').show();
						$("#Login-Name").html("<font color='black' ><strong></strong></font>");
						$sessionData = "";
						
											
						
					},
					error: function (error) {
						
						$("#messageM").html("<font color='black' ><strong></strong></font>");
						$("#messageM").append('Error Connecting to Server');
						$('#messageModal').modal('show');
					}
				});
			});
			
			
			
		});
		</script>
	
	
