			$('#reserverButton').click(function(e){
				$("#msg").html("<font color='black'><strong></strong></font>");
				e.preventDefault();
				var $emp = $('#empID').val();
				var $name = $('#name').val();
				var $from = $('#from');
				var $to = $('#to');
				var $mc = $('#mc').val();
				var $reason = $('#reason').val();
			
			
				var $fDate = new Date($from.val());
				var $tDate = new Date($to.val());
				var $today = new Date();
			
				if($emp < 99999) {
					alertM("Please enter a valid Employee Number");
					return false;
				}
				
				if($name =="" || $name == parseInt($name) || $name =='Enter Full Name' ){
					alertM("Name can not be blank or Numeric");
					return false;
				}
				
				
				
				if(isFinite($fDate) && isFinite($tDate)){
					if($fDate >= $tDate){
						alertM("From Date must be less than To Date");
						return false;
					}
					//console.log(today);
					if($fDate < $today){
						alertM("From Date must be greater than or equal to Today's Date");
						return false;
					}
					$('#from').val($fDate);
					$('#to').val($tDate);
				}
				else {
					alertM("Date(s) not in correct format or not chosen correctly");
					return false;
				}
				
				if($mc < 5 || $mc > 14){
					alertM("Please Choose a machine Number");
					return false;
				}
				
				
				
				if($reason ==""){
					alertM("Please fill the reason");
					return false;
				}
				
				var btn = $(this);
				btn.button('loading');
				
				
				var data = {};
				data.emp = $emp;
				data.name = $name;
				data.from = $fDate;
				data.to = $tDate;
				data.mc = $mc;
				data.reason = $reason;
				
				$.ajax({
					type: 'POST',
					data: JSON.stringify(data),
					contentType: 'application/json',
					url: '/doReservation',						
					success: function(data) {
						
						$("#messageM").html("<font color='black' ><strong></strong></font>");
						$("#messageM").append("<center><font color='black' face='trebuchet MS' size='5' >"+data.message+"</font></center>");
